(){\r\n        history.replace(`/shop/${productID}`)\r\n    }\r\n\r\n    scbService.setCallBack(link)\r\n\r\n    async function buyItem(){\r\n        window.location = (await scbService.createLink(500)).deeplinkUrl\r\n        db.ref(`/users/${auth.currentUser.uid}/download/${productID}/`).set({\r\n            link: link,\r\n            name: name\r\n        })\r\n    }\r\n\r\n    return(\r\n        <div>\r\n            <p></p>\r\n            <a onClick={buyItem}>COMFIRM BUY</a>\r\n            <button onClick={goBack}>BACK</button>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Buy","D:\\devcamp\\src\\ProductDetail.js",["86"],{"ruleId":"87","severity":1,"message":"88","line":3,"column":10,"nodeType":"89","messageId":"90","endLine":3,"endColumn":14},{"ruleId":"91","severity":1,"message":"92","line":49,"column":13,"nodeType":"93","endLine":49,"endColumn":65},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","severity":1,"message":"99","line":27,"column":7,"nodeType":"100","endLine":27,"endColumn":62},{"ruleId":"91","severity":1,"message":"92","line":35,"column":9,"nodeType":"93","endLine":35,"endColumn":102},{"ruleId":"87","severity":1,"message":"101","line":9,"column":5,"nodeType":"89","messageId":"90","endLine":9,"endColumn":21},{"ruleId":"87","severity":1,"message":"102","line":10,"column":5,"nodeType":"89","messageId":"90","endLine":10,"endColumn":19},{"ruleId":"98","severity":1,"message":"99","line":29,"column":7,"nodeType":"100","endLine":29,"endColumn":57},{"ruleId":"91","severity":1,"message":"92","line":37,"column":17,"nodeType":"93","endLine":37,"endColumn":106},{"ruleId":"87","severity":1,"message":"103","line":11,"column":5,"nodeType":"89","messageId":"90","endLine":11,"endColumn":19},{"ruleId":"87","severity":1,"message":"104","line":13,"column":5,"nodeType":"89","messageId":"90","endLine":13,"endColumn":20},{"ruleId":"87","severity":1,"message":"105","line":14,"column":5,"nodeType":"89","messageId":"90","endLine":14